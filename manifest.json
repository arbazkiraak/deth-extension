{
  "manifest_version": 3,
  "name": "DethEtherscan",
  "version": "1.0.1",
  "description": "dETH Code explorer inside etherscan",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://etherscan.io/*",
        "https://arbiscan.io/*",
        "https://polygonscan.com/*",
        "https://bscscan.com/*",
        "https://optimistic.etherscan.io/*"
      ],
      "js": [
        "Content.js"
      ]
    }
  ]
}
